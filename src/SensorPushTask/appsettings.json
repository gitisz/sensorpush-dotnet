{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
"InfluxDBv2": {
    "Protocol": "http",
    "Url": "influxdb",
    "Port": 8086,
    "TokenKey": "INFLUX_DB_V2_TOKEN",
    "OrgKey": "INFLUX_DB_V2_ORG",
    "Bucket": "sensorpush_dotnet"
  },
  "SensorPush": {
    "Interval": 1,
    "Limit": 20,
    "Login": {
      "UserNameKey": "USERNAME",
      "PasswordKey": "PASSWORD"
    },
        "Measures": [
      "temperature",
      "humidity",
      "vpd",
      "barometric_pressure",
      "dewpoint"
    ]
  }
}